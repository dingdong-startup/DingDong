 <%if agency_signed_in? %>

<%= render 'layouts/navbar' %>

<%= render 'layouts/property_show_agency_banner', property: @property, areas: @areas %>
<%= render 'layouts/property_show_visitors', visits: @asked_visits %>
<%= render 'layouts/property_show_description', agency: @agency, property: @property%>
<%= render 'layouts/property_show_carroussel', property: @property %>

